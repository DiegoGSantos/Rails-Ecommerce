 <% @products.each do |product| %>
  <div class="col-md-4 text-center">
    <div class="thumbnail">
      <%= image_tag product.image.url(), class: "img-responsive" %>
      <div class="caption">
        <h3><%= product.name %>
            <br>
            <small><%= product.description %></small>
        </h3>
        <span>
            <i class="fa fa-star fa-lg"></i>
            <i class="fa fa-star fa-lg"></i>
            <i class="fa fa-star fa-lg"></i>
            <i class="fa fa-star fa-lg"></i>
            <i class="fa fa-star-o fa-lg"></i>
        </span> 
      </div>
    </div>
  </div>
  <% end %>